{
  "name": "unison",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_toplevel"
      }
    },
    "_toplevel": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "use_statement"
        },
        {
          "type": "SYMBOL",
          "name": "declaration"
        }
      ]
    },
    "uppercase_identifier": {
      "type": "PATTERN",
      "value": "([A-Z]|\\p{Emoji_Presentation})([A-Za-z0-9_!']|\\p{Emoji_Presentation})*"
    },
    "lowercase_identifier": {
      "type": "PATTERN",
      "value": "([a-z]|\\p{Emoji_Presentation})([A-Za-z0-9_!']|\\p{Emoji_Presentation})*"
    },
    "regular_identifier": {
      "type": "PATTERN",
      "value": "([A-Za-z_]|\\p{Emoji_Presentation})([A-Za-z0-9_!']|\\p{Emoji_Presentation})*"
    },
    "operator": {
      "type": "PATTERN",
      "value": "[!$%^&*\\-=+<>.~\\\\/|:]+"
    },
    "identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "regular_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        }
      ]
    },
    "qualified_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "regular_identifier"
              },
              {
                "type": "STRING",
                "value": "."
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "qualified_operator": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "regular_identifier"
              },
              {
                "type": "STRING",
                "value": "."
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "operator"
        }
      ]
    },
    "use_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_start_mark"
        },
        {
          "type": "STRING",
          "value": "use"
        },
        {
          "type": "SYMBOL",
          "name": "qualified_identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_end_mark"
        }
      ]
    },
    "declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_start_mark"
        },
        {
          "type": "SYMBOL",
          "name": "qualified_identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "SYMBOL",
          "name": "_end_mark"
        }
      ]
    },
    "type_variable": {
      "type": "SYMBOL",
      "name": "lowercase_identifier"
    },
    "type_constructor": {
      "type": "SYMBOL",
      "name": "uppercase_identifier"
    },
    "type_unit": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "type_operator": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "qualified_operator"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "type": {
      "type": "SYMBOL",
      "name": "_type2"
    },
    "_type1": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "type_variable"
          },
          {
            "type": "SYMBOL",
            "name": "type_constructor"
          },
          {
            "type": "SYMBOL",
            "name": "type_unit"
          },
          {
            "type": "SYMBOL",
            "name": "type_operator"
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "ability"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "_type1"
              }
            ]
          },
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "("
              },
              {
                "type": "SYMBOL",
                "name": "_type_or_tuple_contents"
              },
              {
                "type": "STRING",
                "value": ")"
              }
            ]
          }
        ]
      }
    },
    "_type_or_tuple_contents": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "type"
              }
            ]
          }
        }
      ]
    },
    "_type2": {
      "type": "PREC_RIGHT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_type1"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "qualified_operator"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "ability"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "_type1"
                }
              ]
            }
          }
        ]
      }
    },
    "ability": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "type"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    },
    {
      "type": "STRING",
      "value": "\t"
    },
    {
      "type": "STRING",
